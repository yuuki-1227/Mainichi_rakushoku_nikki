  <table>
    <% end_users.each do |end_user| %>
      <% if end_user.is_deleted == false %>
        <tr>
          <td>
            <%= link_to end_user_path(end_user) do %>
            <%= attachment_image_tag end_user, :profile_image, :fill, 100, 100, fallback: "no_image.jpg" %>
            <% end %>
          </td>
          <td>
            <%= end_user.last_name + end_user.first_name %></br>
            <%= end_user.introduction %>
          </td>
        </tr>
      <% end %>
    <% end %>
  </table>